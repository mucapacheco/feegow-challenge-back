<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

  <entity name="Domain\Entity\Agendamento" table="agendamento">
    <id name="id" type="integer" column="user_id">
      <generator strategy="AUTO" />
    </id>
    <field name="specialty_id"    column="specialty_id"     nullable="false" type="integer"    />
    <field name="professional_id" column="professional_id"  nullable="false" type="integer"  />
    <field name="name"            column="name"             nullable="false" type="string"  />
    <field name="cpf"             column="cpf"              nullable="false" type="string"  />
    <field name="source_id"      column="source_id"       nullable="false" type="integer"  />
  </entity>
</doctrine-mapping>